<odoo>
  <data noupdate="1">
    <record id="ir_module_module_restrict_rule" model="ir.rule">
      <field name="name">Restringir acceso a Aplicaciones</field>
      <field name="model_id" ref="base.model_ir_module_module"/>
      <field name="groups" eval="[(4, ref('exe_restriction_user_16.group_no_permission'))]"/>
      <field name="domain_force">[('id', '=', -1)]</field>
      <field name="perm_read" eval="True"/>
      <field name="perm_write" eval="False"/>
      <field name="perm_create" eval="False"/>
      <field name="perm_unlink" eval="False"/>
    </record>
    <record id="board_board_restrict_rule" model="ir.rule">
      <field name="name">Restringir acceso a Tableros</field>
      <field name="model_id" ref="board.model_board_board"/>
      <field name="groups" eval="[(4, ref('exe_restriction_user_16.group_no_permission'))]"/>
      <field name="domain_force">[('id','=',False)]</field>
      <field name="perm_read" eval="True"/>
      <field name="perm_write" eval="False"/>
      <field name="perm_create" eval="False"/>
      <field name="perm_unlink" eval="False"/>
    </record>
